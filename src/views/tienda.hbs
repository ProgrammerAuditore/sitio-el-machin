<style>
  .img-size {
    width: 100%;
    height: 400px;
  }
</style>
<!-- Start Content -->
<div class="container py-5">
  <div class="row">   
    <div class="col-lg-12">
      <div class="row">

        {{#each datos}}
        <div class="col-md-4">
          <div class="card mb-4 productos-hamburgesa rounded-0">
            <div class="card rounded-0">
              <img
                class="card-img rounded-0 img-fluid img-size"
                src="img/{{cmpImagen}}"
              />
              <div
                class="card-img-overlay rounded-0 product-overlay d-flex align-items-center justify-content-center"
              >
              </div>
            </div>
            <div class="card-body">
              <a
                href="shop-single.html"
                class="h3 text-decoration-none"
              >{{cmpNombre}}</a>              
              <ul
                class="w-100 list-unstyled d-flex justify-content-between mb-0"
              >
                <li>{{cmpMarca}}</li>
                <li>${{cmpPrecio}}</li>
              </ul>
            </div>
          </div>
        </div>
        {{/each}}

      </div>
    </div>

  </div>
</div>
<!-- End Content -->
